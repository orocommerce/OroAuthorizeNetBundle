{
  "name": "oro/commerce-authorizenet",
  "description": "OroCommerce Authorize.Net integration",
  "homepage": "https://github.com/orocommerce/OroAuthorizeNetBundle.git",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Oro, Inc",
      "homepage": "https://www.orocommerce.com"
    }
  ],
  "autoload": {
    "psr-4": {"Oro\\Bundle\\AuthorizeNetBundle\\": ""},
    "exclude-from-classmap": ["/Tests/"]
  },
  "require": {
    "oro/commerce": "4.0.*",
    "authorizenet/authorizenet": "~1.9.2"
  },
  "repositories": {
    "oro": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.orocrm.com"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  }
}
